{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-any": false,
        "ordered-imports": false,
        "no-empty-interface": true,
        "no-console": [true, "log", "error"],
        "no-duplicate-variable": true,
        "prefer-object-spread": true,
        "prefer-const": true,
        "class-name": true,
        "whitespace": [true, "check-branch", "check-operator", "check-typecast"]
    },
    "rulesDirectory": []
}